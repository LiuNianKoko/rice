<view><block wx:for="{{list}}" wx:for-item="data" wx:for-index="idx" wx:key="idx"><view><view class="title">{{'请上传'+data.title+'照片'}}</view><view class="img-box"><block wx:for="{{imgArr[idx].img}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="img-wrap"><image class="del" src="/static/del.png" data-event-opts="{{[['tap',[['delImg',[idx,index]]]]]}}" bindtap="__e"></image><image class="imgs" src="{{item.file}}" data-event-opts="{{[['tap',[['previewImg',[idx,index]]]]]}}" bindtap="__e"></image></view></block><view class="img-wrap"><image class="imgs" src="/static/add.png" data-event-opts="{{[['tap',[['chooseImage',['$0',idx],[[['list','',idx]]]]]]]}}" bindtap="__e"></image></view></view><view class="num-wrap scale-1px-bottom">{{''+(imgArr[idx].img.length||0)+" / "+data.num+''}}</view></view></block><view class=" disCenter"><view data-event-opts="{{[['tap',[['upClick',['$event']]]]]}}" class="nexts font24 text-white disCenter" bindtap="__e">第二步，上传照片</view></view><q-s-popup class="vue-ref" vue-id="d5b91fe2-1" data-ref="QSPopup" bind:__l="__l" vue-slots="{{['default']}}"><view class="sure-content"><view class="sure-title">请确认</view><view class="disCenter"><image class="photo-icon" src="/static/photo.png"></image></view><view class="tips">{{'您总共可上传'+allNum+"张图片，当前已上传"+nowNum+'张是否确认上传'}}</view><view class="btns-wrap"><view data-event-opts="{{[['tap',[['hides',['QSPopup']]]]]}}" class="btns disCenter btns-left" bindtap="__e">继续上传</view><view data-event-opts="{{[['tap',[['sureClick',['$event']]]]]}}" class="btns disCenter btns-right" bindtap="__e">确认提交</view></view></view></q-s-popup><block wx:if="{{hids}}"><view data-event-opts="{{[['tap',[['hid',['$event']]]]]}}" class="hids" bindtap="__e"></view></block><block wx:if="{{hids}}"><view data-event-opts="{{[['tap',[['hid',['$event']]]]]}}" class="hids-main" bindtap="__e"><canvas class="firstCanvas" style="width:300px;height:420px;" canvas-id="firstCanvas"></canvas></view></block><q-s-popup class="vue-ref" vue-id="d5b91fe2-2" data-ref="QSPhoto" bind:__l="__l" vue-slots="{{['default']}}"><view class="photo-wrap"><image class="photos" src="{{src}}"></image><image class="del-white" src="/static/del-white.png" data-event-opts="{{[['tap',[['hides',['QSPhoto']]]]]}}" bindtap="__e"></image></view></q-s-popup></view>