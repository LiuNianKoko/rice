<view class="content" style="{{'height:'+(screenHeight)+';'}}"><view class="tops"><view class="left"><image class="headImage" mode src="{{userData.headImage}}" data-event-opts="{{[['tap',[['goSet',['$event']]]]]}}" bindtap="__e"></image></view><view class="center"><view class="name top overflow_one">{{userData.name}}</view><view class="name bottom overflow_one">{{"用户id:"+userData.id}}</view></view><view data-event-opts="{{[['tap',[['backHome',['$event']]]]]}}" class="back" bindtap="__e">返回首页</view><image class="bottomImage" src="../../static/image/user/bottom.png" mode data-event-opts="{{[['tap',[['topImage',['$event']]]]]}}" bindtap="__e"></image></view><view class="orderBox"><view class="title"><view class="titleName">我的订单</view><view data-event-opts="{{[['tap',[['orderBtn',[-1]]]]]}}" class="right" bindtap="__e">查看全部<uni-icons class="icons" vue-id="380011e0-1" type="arrowright" color="rgb(51,51,51)" size="12" bind:__l="__l"></uni-icons></view></view><view class="clearBoth"></view><view class="orderBtns"><block wx:for="{{orderData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['orderBtn',[index]]]]]}}" class="boxs" style="{{(index==1?{'width':'60%'}:{'width':'20%'})}}" bindtap="__e"><image class="images" src="{{item.src}}" mode></image><view class="name">{{item.name}}</view></view></block><view class="clearBoth"></view></view></view><view class="toolsBox"><block wx:for="{{toolsData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toolsBtn',[index]]]]]}}" class="rows" bindtap="__e"><image class="iconLefts" src="{{item.src}}" mode></image><view class="name">{{item.name}}</view><uni-icons class="icons" vue-id="{{'380011e0-2-'+index}}" type="arrowright" color="rgb(197,197,197)" size="12" bind:__l="__l"></uni-icons><view class="clearBoth"></view></view></block></view></view>