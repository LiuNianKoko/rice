
.content{width:100%;height:100vh;background-image:url(/static/image/user/dividends/bg_1.png);background-size: 100% 115%;background-repeat: no-repeat;}
.content .btnGo{width: 100%;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;margin-top: 230rpx;}
.content .btnGo .btnGos{width:120rpx;border:1px solid white;color:white;border-radius: 10rpx;font-size: 36rpx;text-align: center;}
.content .rowsBox{width: 90%;margin-left: 5%;margin-top: 100rpx;margin-bottom: 0rpx;}
.content .rowsBox .rows{width:40%;margin:0 2.5%;padding:2.5%;background: white;border-radius: 10rpx;float: left;}
.content .rowsBox .rows .rowsDisplay{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;}
.content .rowsBox .rows .price{color:#E43E40;font-size: 36rpx;text-align: center;font-weight: bold;}
.content .rowsBox .rows .tops{color:white;background: #E43E40;position: relative;left:10rpx;border-radius: 100rpx;padding:0rpx 15rpx;font-size: 12px;text-align: center;margin:10rpx 0;}
.content .rowsBox .rows .name{color:#9A9A9A;font-size:14px;text-align: center;margin-top: 20rpx;}
.content .rowsBox .rows .icons{width:50rpx;height:50rpx;float: left;margin-right:10rpx}
.content .rowsBox .rows .names{float: left;font-size: 12px;margin-top:10rpx}
.content .rowsBox .rows .prices{float: left;font-size: 12px;color:#E43E40;margin-top:10rpx}
.content .row{width:85%;padding:20rpx 0;margin-left: 7.5%;background: white;border-radius: 10rpx;margin-top:20rpx;}
.content .row .icons{width:50rpx;height:50rpx;float: left;margin-left: 2.5%;margin-right:10rpx}
.content .row .names{float: left;font-size: 12px;margin-top:10rpx}
.content .row .prices{float: right;font-size: 12px;color:#E43E40;margin-top:10rpx;margin-right: 2.5%;}
.content .last{width:75%;margin-left: 7.5%;padding:4% 5%;margin-top: 20rpx;background: white;border-radius: 10rpx;}
.content .last .last_title{font-size: 30rpx;color:#343434;margin-bottom: 20rpx;font-weight: bold;}
.content .last .last_rows{font-size: 26rpx;color:#666666;margin:20rpx 0;}
.content .last .btnImage{width:80%;height:80rpx;margin-left: 10%;margin-top: 40rpx;}


