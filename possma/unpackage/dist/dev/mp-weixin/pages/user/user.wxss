
.statusBox{width:100%;height:90rpx;background: white;position: fixed;top:0;left: 0;z-index: 2;}
.content{padding-bottom: 0rpx;overflow-x: hidden;background:rgb(244,244,244);height:100%;}
.content .tops{width:100%;position: relative;display: -webkit-box;display: -webkit-flex;display: flex;padding-top:50rpx;padding-bottom: 180rpx;background:url(data:image/png;base64,);background-repeat: no-repeat;background-size: 100% 100%;}
.content .tops .left{width:auto;float: left;margin-left: 5%;}
.content .tops .left .headImage{width:108rpx;height:108rpx;border-radius: 108rpx;float: left;}
.content .center{width:55%;margin:5rpx 0 0 20rpx;}
.content .center .name{color:white;}
.content .center .top{font-size:18px;}
.content .center .bottom{font-size:12px;margin-top:15rpx;}
.content .back{background: white;color:rgb(78,178,244);padding:0 20rpx;border-radius: 100rpx;height:50rpx;line-height: 50rpx;font-size: 12px;margin-left: 3%;margin-top: 20rpx;}
.content .bottomImage{position: absolute;width:90%;height:160rpx;bottom:-10%;left:5%}
.content .orderBox{width:90%;padding:2.5%;margin-left: 2.5%;margin-top:80rpx;background: white;border-radius: 5rpx;}
.content .orderBox .titleName{float: left;color: #000000;font-weight: bold;}
.content .orderBox .right{float: right;font-size: 12px;}
.content .orderBox .right .icons{position: relative;top:4rpx}
.content .orderBtns{width:100%;display: -webkit-box;display: -webkit-flex;display: flex;background: white;border-top:1px dashed rgb(136,136,136);margin-top: 20rpx;}
.content .orderBtns .boxs{-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;margin-top:30rpx;text-align: center;}
.content .orderBtns .boxs .images{width:55rpx;height:50rpx;}
.content .orderBtns .boxs .name{font-size: 12px;}
.content .toolsBox{width:95%;background:white;border-radius: 10rpx;margin-top: 40rpx;margin-left: 2.5%;}
.content .toolsBox .rows{width:95%;border-bottom: 1px solid rgb(242,242,242);padding:4%;padding-left: 5%;}
.content .toolsBox .rows .iconLeft{width:30rpx;height:34rpx;float: left;position: relative;top:4rpx;margin-right:20rpx}
.content .toolsBox .rows .iconLefts{width:28rpx;height:34rpx;float: left;position: relative;top:4rpx;margin-right:20rpx}
.content .toolsBox .rows .name{font-size: 14px;margin:0 10rpx;float: left;}
.content .toolsBox .rows .icons{float: right;margin-right: 2%;}




