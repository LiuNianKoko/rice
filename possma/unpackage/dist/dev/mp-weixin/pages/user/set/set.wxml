<view class="content"><block wx:for="{{setData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['setBtn',[index]]]]]}}" class="rows" bindtap="__e"><block wx:if="{{index==0}}"><image class="headImage" src="{{item.src}}" mode></image></block><block wx:if="{{index!==0}}"><view class="names">{{item.name}}</view></block><uni-icons class="icons" style="{{(index==0?{'top':'30upx'}:'')}}" vue-id="{{'5bdc36dc-1-'+index}}" type="arrowright" color="#939393" size="12" bind:__l="__l"></uni-icons><view class="rightName overflow_one" style="{{(index==0?{'height':'92upx','lineHeight':'92upx'}:'')}}">{{item.rightName}}</view><view class="clearBoth"></view></view></block><view data-event-opts="{{[['tap',[['outBtn',['$event']]]]]}}" class="out" bindtap="__e">退出登录</view><block wx:if="{{showUpDate}}"><view class="showShadow"><view data-event-opts="{{[['tap',[['closeShadow',['$event']]]]]}}" class="close" bindtap="__e"></view><view class="bg_white"><image class="update" src="../../../static/image/user/set/update.png" mode></image><block wx:if="{{update==true}}"><view class="boxes">{{versionTitle}}</view></block><block wx:if="{{update==true}}"><view class="boxes">建议立即更新</view></block><block wx:if="{{update==false}}"><view class="boxes">您当前已是最新版本</view></block><block wx:if="{{update==false}}"><view class="boxes">无需更新</view></block><block wx:if="{{update==true}}"><view data-event-opts="{{[['tap',[['closeShadow',['$event']]]]]}}" class="btn leftBtn" bindtap="__e">取消</view></block><block wx:if="{{update==true}}"><view data-event-opts="{{[['tap',[['downLoadNow',['$event']]]]]}}" class="btn rightBtn" bindtap="__e">确认更新</view></block><block wx:if="{{update==false}}"><view data-event-opts="{{[['tap',[['closeShadow',['$event']]]]]}}" class="btn sure" bindtap="__e">确认</view></block><view class="clearBoth"></view></view></view></block><block wx:if="{{showShadow}}"><view class="showDow"><view class="bg_grey"></view><view class="bg_white"><view class="title">{{titleName}}</view><input class="contents fontSize14 textCenter" type="text" placeholder="请输入" data-event-opts="{{[['input',[['getInputValue',['$event']]]]]}}" value="{{inputVlaue}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['clickFlase',['$event']]]]]}}" class="btns falseBtn" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['clickSure',['$event']]]]]}}" class="btns sureBtn" bindtap="__e">确认</view></view></view></block></view>