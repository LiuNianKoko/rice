
.content{overflow-y: hidden;}
.content .title{margin-left: 5%;margin-top:80rpx;font-size:18px;font-weight: bold;}
.content .inputBox{width:90%;margin-left: 5%;margin-top: 40rpx;font-size: 20px;border-bottom: 1px solid rgb(246,246,246)}
.content .name{margin-top: 40rpx;font-size: 15px;float: left;margin-left: 5%;}
.content .num{margin-top: 40rpx;font-size: 15px;float: left;margin-left:2%;color:rgb(79,176,244)}
.content .all{margin-top: 40rpx;font-size: 16px;float: right;margin-right:5%;color:rgb(223,40,47);margin-left: 5%;}
.content .tips{margin-top: 20rpx;font-size: 14px;float: left;margin-left: 5%;}
.content .next{width:80%;margin-left: 5%;padding:3% 5%;margin-top:200rpx;background: rgb(79,176,244);color:white;text-align: center;border-radius: 100rpx;}
.content .shaDow{width:100%;height:100%;position: fixed;top:0;left:0}
.content .shaDow .close{width:100%;height:100%;position: fixed;top:0;left:0;background: rgba(0,0,0,0.4);}
.content .shaDow .bg_white{width:60%;padding:5%;position: fixed;top:40%;left:15%;background:white;border-radius: 10rpx;}
.content .shaDow .bg_white .box{width:50%;text-align: center;float: left;}
.content .shaDow .bg_white .box .image{width:50rpx;height: 50rpx;}
.content .shaDow .bg_white .box .names{margin-top:10rpx;font-size:15px;}
.content .shaDow .bg_white .false{letter-spacing: 4rpx;border-top:1px solid rgb(229,229,229);padding-top: 20rpx;text-align: center;color:#959595;font-size:17px;margin-top: 70rpx;}

