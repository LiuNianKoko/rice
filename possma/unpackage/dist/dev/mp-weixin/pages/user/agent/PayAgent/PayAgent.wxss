
.content{overflow-y: hidden;}
.content .footer{position: absolute;left:0;bottom: 0;width:100%;height:100rpx;line-height: 100rpx;border-top: 1px solid rgba(0,0,0,0.2);}
.content .footer .nameA{float: left;font-size: 14px;margin-left:20rpx}
.content .footer .btnNow{float: right;font-size:14px;background: rgb(79,176,244);color:white;border-radius: 200rpx;text-align: center;margin-right: 20rpx;height:50rpx;line-height: 50rpx;margin-top: 25rpx;padding:5rpx 30rpx;}
.content .shaDow{width:100%;height:100%;position: fixed;top:0;left:0}
.content .shaDow .close{width:100%;height:100%;position: fixed;top:0;left:0;background: rgba(0,0,0,0.4);}
.content .shaDow .bg_white{width:60%;padding:5%;position: fixed;top:40%;left:15%;background:white;border-radius: 10rpx;}
.content .shaDow .bg_white .box{width:50%;text-align: center;float: left;}
.content .shaDow .bg_white .box .image{width:50rpx;height: 50rpx;}
.content .shaDow .bg_white .box .names{margin-top:10rpx;font-size:15px;}
.content .shaDow .bg_white .false{letter-spacing: 4rpx;border-top:1px solid rgb(229,229,229);padding-top: 20rpx;text-align: center;color:#959595;font-size:17px;margin-top: 70rpx;}
.content .rows{width:90%;padding:2.5%;margin:20rpx 0;margin-left:2.5%;border-radius: 10rpx;border:1px solid rgba(0,0,0,0.1);}
.content .rows .icon{float: left;width:32rpx;height:46rpx;line-height: 46rpx;margin:0 10rpx}
.content .rows .iconsLeft{width:33rpx;height:32rpx;position: relative;top:6rpx;}
.content .rows .name{float: left;font-size: 14px;color:#ABABAB;height:46rpx;margin-left: 10rpx;line-height: 46rpx;}
.content .rows .icons{float: right;position: relative;top:10rpx}

