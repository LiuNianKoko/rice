
.content{}
.content .bg{width:100%;height:426rpx}
.content .rows{width:90%;padding:2.5%;margin-left: 2.5%;margin-top: 20rpx;}
.content .rows .iconBox{width:10%;height:80rpx;float: left;background: rgb(169,214,234);display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;border-top-left-radius: 10rpx;border-bottom-left-radius: 10rpx;}
.content .rows .iconBox .icon{width:50rpx;height:50rpx;margin-top: 15rpx;}
.content .rows .right{width:90%;height:80rpx;line-height:80rpx;float: left;background: rgb(227,245,255);border-top-right-radius: 10rpx;border-bottom-right-radius: 10rpx;}
.content .rows .right .name{width:50%;color:#333333;margin-left: 30rpx;float: left;}
