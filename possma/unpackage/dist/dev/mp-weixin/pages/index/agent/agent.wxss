
.content .isTrue{width:95%;margin-left: 5%;}
.content .isTrue .icon{width:28rpx;height:28rpx;float: left;position: relative;top:6rpx}
.content .isTrue .name{float: left;color:#535353;margin-left: 20rpx;}
.content .topBox{width:100%;height:330rpx;position: relative;background-image: url(/static/image/user/agent/icon_1.png);background-repeat: no-repeat;background-size: 100% 100%;}
.content .topBox .tip{width:50%;background: rgb(138,179,255);color:white;border-radius: 10rpx;text-align: center;padding:10rpx 0;position: absolute;top:30%;right:10%}
.content .topBox .tips{width:50%;color: rgb(138,179,255);border-radius: 10rpx;text-align: center;padding:10rpx 0;position: absolute;top:50%;right:10%;font-size: 13px;}
.content .splites{width:100%;height:20rpx;background: rgb(244,244,244);}
.content .title{width:90%;padding:3% 2.5%;margin-left: 2.5%;position: relative;}
.content .title .titleName{font-weight:bold;}
.content .littleTitle{color:#535353;margin:20rpx 0;margin-left: 5%;}
.content .title .splite{width:40rpx;height:4rpx;background: rgb(78,178,244);margin-top:10rpx;border-radius: 20rpx;}
.content .rows{width:90%;padding:3% 2.5%;margin-bottom: 80rpx;margin-left: 2.5%;}
.content .rows .icon{width:25rpx;height:25rpx;float: left;position: relative;top:10rpx}
.content .rows .right{width:85%;margin-left:3%;float: left;display:-webkit-box;display:-webkit-flex;display:flex;}
.content .rows .right .name{text-align:left;font-size:13px;color:#4CA6D5;display: inline;}
.content .rows .right .names{text-align:left;font-size:13px;color:#434343;display: inline;}
.content .rows .right .contents{text-align:left;font-size: 13px !important;color:#7D7D7D;display: inline;}
.content .next{width:90%;padding:3% 2.5%;margin-bottom: 80rpx;margin-left: 2.5%;text-align: center;color: white;background: rgb(78,178,244);border-radius: 100rpx;}
.content .shaDow{width:100%;height:100%;position: fixed;top:0;left:0}
.content .shaDow .close{width:100%;height:100%;position: fixed;top:0;left:0;background: rgba(0,0,0,0.4);}
.content .shaDow .bg_white{width:60%;padding:5%;position: fixed;top:40%;left:15%;background:white;border-radius: 10rpx;}
.content .shaDow .bg_white .box{width:50%;text-align: center;float: left;}
.content .shaDow .bg_white .box .image{width:50rpx;height: 50rpx;}
.content .shaDow .bg_white .box .names{margin-top:10rpx;font-size:15px;}
.content .shaDow .bg_white .false{letter-spacing: 4rpx;border-top:1px solid rgb(229,229,229);padding-top: 20rpx;text-align: center;color:#959595;font-size:17px;margin-top: 70rpx;}
.content .html{width:90%;padding:2.5%;margin-left: 2.5%;}

