
.statusBox{width:100%;height:90rpx;background: white;position: fixed;top:0;left: 0;z-index: 2;}
.content{width:100%}
.content .logo{width:25%;height: 187.5rpx;margin-left:37.5%;margin-top:120rpx;}
.content .passwordBox{width:100%;position: relative;}
.content .passwordBox .eye{width:40rpx;height: 25rpx;position: absolute;top:40%;right:10%;z-index: 2;}
.content .input{width:85%;margin-left:5%;padding:2.5%;border-bottom: 1px solid rgb(240,240,240);}
.content .getCode{position: absolute;top:20%;right:8%;color:rgb(78,178,244);text-align: center;}
.content .marginTop200{margin-top:200rpx;}
.content .marginTop20{margin-top:20rpx}
.content .uni-input-placeholder{font-size: 14px;color:#A5A5A5;}
.content .loginBtn{width:90%;margin-left: 5%;background: rgb(78,178,244);color:white;border-radius: 200rpx;text-align: center;padding:2.5% 0;margin-top:100rpx;}
.content .btns{width:100%;margin-top: 20rpx;}
.content .btns .register{width:40%;margin-left: 9%;text-align: left;float:left;color:#434343}
.content .btns .forget{width:40%;margin-right: 9%;text-align: right;float: right;color:#C41E3F}
.content .loginBtns{width:100%;margin-top:150rpx;float: left;}
.content .loginBtns .box{width:50%;float: left;text-align: center;}
.content .loginBtns .box .iconImage{width:70rpx;height: 60rpx;}
.content .loginBtns .box .iconImages{width:40rpx;height: 60rpx;}
.content .loginBtns .box .name{color:#2F2F2F;margin-top: 10rpx;}
.content .last{position: absolute;left:0;bottom: 1%;text-align: center;width:100%;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;}
.content .last .common{float:left;}
.content .last .other{float:left;color:#4FB0F4;}


