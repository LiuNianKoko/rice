.wrap {
  padding: 0 38rpx;
}
.item {
  padding: 36rpx 0;
  border-bottom: 1px solid #f1f1f1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.item .item-left {
  color: #434343;
  font-size: 27rpx;
  width: 180rpx;
  font-weight: bold;
}
.item .item-right {
  font-size: 26rpx;
}
.de-wrap {
  padding: 24rpx 0;
}
.de-wrap .de-txt {
  color: #9D9D9D;
  font-size: 26rpx;
}
.btn {
  margin-top: 160rpx;
  height: 90rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #EC7A8E;
  border: 1px solid #EC7A8E;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  text-align: center;
  line-height: 90rpx;
}

