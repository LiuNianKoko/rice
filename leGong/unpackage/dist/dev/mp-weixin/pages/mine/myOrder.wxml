f="{{item.after.status==9}}"><view>退款中</view></block><block wx:if="{{item.after.status==10}}"><view>已终止</view></block><block wx:if="{{item.after.status==0}}"><view>已撤销</view></block></view><view>{{''+item.after.create_time+''}}</view></view></block><view data-event-opts="{{[['tap',[['toOrderDetail',['$0'],[[['list','',index]]]]]]]}}" class="mid-box disFlex" bindtap="__e"><image class="imgs" src="{{item.goods.thumb_url}}"></image><view class="mid-right"><view class="goods-num fontBold">{{'*'+item.goods.goods_num+''}}</view><view class="line2 fontBold mid-title">{{''+item.goods.goods_name+''}}</view><view class="font24 color-gray line1">{{''+item.goods.goods_attr_str+''}}</view><view class="disBetween"><view class=" order-price"><text class="font22 fontBold">￥</text><text class="fontBold font30">{{item.goods.price||0}}</text><block wx:if="{{item.goods.unit}}"><text class="fontBold">/</text></block><block wx:if="{{item.goods.num>1&&item.goods.unit}}"><text class="fontBold font24">{{item.goods.num||''}}</text></block><text class="fontBold font24">{{item.goods.unit||''}}</text></view><block wx:if="{{item.shouhou!=1}}"><view><block wx:if="{{item.state==0&&item.status==1&&item.order_type==1||item.status==1&&item.state==0&&item.order_type==2}}"><view class="fontBold color-red">待付款</view></block><block wx:if="{{item.state==1&&item.order_type==2&&item.status==1}}"><view class="fontBold color-red">已付定金</view></block><block wx:if="{{item.status!=1&&item.status!=5&&item.order_type==1}}"><view class="fontBold color-red">已支付</view></block><block wx:if="{{item.status!=1&&item.order_type==2&&item.status!=5}}"><view class="fontBold color-red">已付尾款</view></block></view></block><block wx:if="{{item.shouhou==1}}"><view><view class="fontBold color-red">售后</view></view></block></view></view></view><block wx:if="{{item.order_type==1||item.order_type==4||item.order_type==5}}"><view class="price-box">{{'共'+item.goods_num+"件 合计:￥"+item.total_price+''}}</view></block><block wx:if="{{item.order_type==2||item.order_type==3}}"><view class="price-box-more font26"><view class="price-box-item">{{'共'+item.goods_num+"件   合计：￥"+item.total_price+''}}</view><block wx:if="{{item.state==0&&item.order_type==2&&item.status==1}}"><view class="price-box-item disBetween"><view>剩<text class="color-pinkB">{{item.end_time1||'00:00:00'}}</text>自动关闭</view><view>应付定金：<text class="color-pinkB">{{"￥"+item.deposit}}</text></view></view></block><block wx:if="{{item.state!=0}}"><view class="price-box-item disAlignCenter"><view class="width50">{{'应付定金：￥'+item.deposit+''}}</view><view class="width50">实付定金：<text class="color-pinkB">{{"￥"+item.atc_deposit}}</text></view></view></block><block wx:if="{{item.state!=0}}"><view class="price-box-item disAlignCenter"><view class="width50">{{'应付尾款：￥'+item.balance+''}}</view><block wx:if="{{item.balance_time}}"><view class="width50">实付尾款：<text class="color-pinkB">{{"￥"+item.atc_balance}}</text></view></block></view></block></view></block><block wx:if="{{item.shouhou!=1}}"><view class="btn-box"><block wx:if="{{item.status==1&&item.state==0}}"><view data-event-opts="{{[['tap',[['cancelOrder',['$0',1],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-gray" bindtap="__e">取消订单</view></block><block wx:if="{{item.status==1&&item.state==0&&item.order_type==1}}"><view data-event-opts="{{[['tap',[['toOrderDetail',['$0'],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-red" bindtap="__e">去支付</view></block><block wx:if="{{item.status==1&&item.state==0&&item.order_type==2}}"><view data-event-opts="{{[['tap',[['toOrderDetail',['$0'],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-red" bindtap="__e">支付定金</view></block><block wx:if="{{item.order_type==1&&item.status!=1&&item.status!=5&&item.shouhou==0&&item.shouhou_time==1}}"><view data-event-opts="{{[['tap',[['toApplySale',['$0'],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-red" bindtap="__e">申请售后</view></block><block wx:if="{{item.order_type!=2&&item.status==3}}"><view data-event-opts="{{[['tap',[['cancelOrder',['$0',3],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-border" bindtap="__e">确认收货</view></block><block wx:if="{{item.status==1&&item.check_status==2&&item.order_type==3}}"><view data-event-opts="{{[['tap',[['toOrderDetail',['$0'],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-border" bindtap="__e">支付尾款</view></block><block wx:if="{{item.status==4||item.status==5}}"><view data-event-opts="{{[['tap',[['cancelOrder',['$0',6],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-gray" bindtap="__e">删除订单</view></block></view></block><block wx:if="{{item.shouhou==1}}"><view class="btn-box"><view data-event-opts="{{[['tap',[['toSaleDetail',['$0'],[[['list','',index]]]]]]]}}" class="btn disCenter font26 btn-border" bindtap="__e">查看详情</view></view></block></view></block></view></block><block wx:else><view class="no-order"><image class="lg-no-order" src="{{imgUrl+'lg-no-order.png'}}"></image><view class="marginBottom20">暂无相关订单</view><view>先给自己定个小目标，先下一单</view></view></block><block wx:if="{{backTopShow}}"><image class="back-top" src="/static/back-top.png" mode data-event-opts="{{[['tap',[['backTop',['$event']]]]]}}" bindtap="__e"></image></block></view>