<view class="content"><view class="title" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="back-wrap disAlignCenter" bindtap="__e"><image class="back" src="/static/back.png"></image></view><view class="title-txt">我的订单</view></view><view style="{{'height:'+(CustomBar+'px')+';'}}"></view><view class="search-wrap" style="{{'top:'+(CustomBar+'px')+';'}}"><view class="search-box"><image class="search" src="{{imgUrl+'lg-search.png'}}"></image><input class="inputs" type="text" placeholder="请输入搜索关键词" confirm-type="search" data-event-opts="{{[['confirm',[['searchClick',['$event']]]],['input',[['__set_model',['','search','$event',[]]]]]]}}" value="{{search}}" bindconfirm="__e" bindinput="__e"/><block wx:if="{{search}}"><image class="close" src="/static/close.png" data-event-opts="{{[['tap',[['clearSearch',['$event']]]]]}}" bindtap="__e"></image></block></view><view data-event-opts="{{[['tap',[['searchClick',['$event']]]]]}}" class="color-pinkB" bindtap="__e">搜索</view></view><view style="height:106rpx;"></view><view class="disBetween ordertab"><view data-event-opts="{{[['tap',[['loadOrder',[1]]]]]}}" class="{{['tab-item ',(orderType==1)?'active':'']}}" bindtap="__e"><text>全部</text></view><view data-event-opts="{{[['tap',[['loadOrder',[2]]]]]}}" class="{{['tab-item ',(orderType==2)?'active':'']}}" bindtap="__e"><text>待付款</text></view><view data-event-opts="{{[['tap',[['loadOrder',[3]]]]]}}" class="{{['tab-item ',(orderType==3)?'active':'']}}" bindtap="__e"><text>待发货</text></view><view data-event-opts="{{[['tap',[['loadOrder',[4]]]]]}}" class="{{['tab-item ',(orderType==4)?'active':'']}}" bindtap="__e"><text>待收货</text></view><view data-event-opts="{{[['tap',[['loadOrder',[5]]]]]}}" class="{{['tab-item ',(orderType==5)?'active':'']}}" bindtap="__e"><text>已完成</text></view><view data-event-opts="{{[['tap',[['loadOrder',[6]]]]]}}" class="{{['tab-item ',(orderType==6)?'active':'']}}" bi