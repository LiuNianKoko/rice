page {
  background-color: #f4f4f4;
}
.wraps {
  padding: 23rpx;
}
.month-wrap {
  padding: 33rpx 25rpx;
  background: -webkit-gradient(linear, left top, right top, from(#FF99D9), to(#FFAF99));
  background: -webkit-linear-gradient(left, #FF99D9 0%, #FFAF99 100%);
  background: linear-gradient(90deg, #FF99D9 0%, #FFAF99 100%);
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  font-size: 26rpx;
  color: #fff;
}
.month-wrap .right-white {
  width: 13rpx;
  height: 24rpx;
}
.wrap {
  padding: 8rpx 23rpx 23rpx;
}
.month-item {
  padding: 32rpx;
  background-color: #fff;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  margin-bottom: 5rpx;
}
.month-item .level {
  font-size: 24rpx;
  color: #7D7D7D;
}
.month-item .times {
  font-size: 20rpx;
  color: #A8A8A8;
}
.month-item .lg-level {
  width: 30rpx;
  height: 26rpx;
  margin-right: 14rpx;
}
.choose-month {
  width: 575rpx;
  background: #fff;
}
.choose-month .choose-top {
  background: -webkit-gradient(linear, left top, right top, from(#FF99D9), to(#FFAF99));
  background: -webkit-linear-gradient(left, #FF99D9 0%, #FFAF99 100%);
  background: linear-gradient(90deg, #FF99D9 0%, #FFAF99 100%);
  height: 190rpx;
  padding: 44rpx 44rpx 0;
}
.choose-month .choose-top .months {
  font-size: 52rpx;
}
.choose-month .mon-box {
  padding: 50rpx 30rpx 80rpx;
}
.choose-month .mon-box .mon-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.choose-month .mon-box .mon-wrap .mon-item {
  font-weight: bold;
  width: 16%;
  height: 70rpx;
}
.choose-month .mon-box .mon-wrap .mon-item .mon-item-num {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}
.choose-month .mon-box .mon-wrap .mon-item .act {
  color: #fff;
  background-color: #ed7b8f;
  -webkit-border-radius: 6rpx;
          border-radius: 6rpx;
}
.choose-month .mon-box .mon-wrap .mon-item .act-gra {
  color: #aaa;
}
.choose-month .mon-box .year-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.choose-month .mon-box .year-wrap .year-item {
  width: 50%;
  height: 70rpx;
  font-weight: bold;
}
.choose-month .mon-box .year-wrap .year-item .year-item-num {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}
.choose-month .mon-box .year-wrap .year-item .act {
  color: #fff;
  background-color: #ed7b8f;
  -webkit-border-radius: 6rpx;
          border-radius: 6rpx;
}
.choose-month .mon-box .btn-wrap {
  margin-top: 50rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.choose-month .mon-box .btn-wrap .btn {
  color: #1C2732;
  margin-left: 80rpx;
}

