/*每个页面公共css */
.clearFloat{
 	zoom: 1;
}
.clearFloat::after {
   content: "";
   display: block;
   height: 0;
   clear: both;
   visibility: hidden;
}
.overHidden{
 	overflow: hidden;
}
.hids{
 	position: fixed;
 	width: 100%;
 	height: 100%;
 	left: 0;
 	top: 0;
 	background-color: #000;
 	opacity: .5;
 	z-index: 180;
}
.scale-1px-bottom{
 	position: relative;
 	border: none;
}
.scale-1px-bottom::after{
 	content: '';
 	position: absolute;
 	bottom: 0;
 	right: 0;
 	background: #f2f2f5;
 	height: 1px;
 	width: 100%;
 	-webkit-transform: scaleY(0.5);
 	transform: scaleY(0.5);
 	-webkit-transform-origin: 0 0;
 	transform-origin: 0 0;
}
.scale_border{
 	position: relative;
 	border: none;
}
.scale_border::after{
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    height: 200%;
    width: 200%;
    border: 1px solid #f3f1f1;
    -webkit-border-radius: 20rpx;
            border-radius: 20rpx;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
