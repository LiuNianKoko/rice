page {
  padding: 0 20rpx;
}
.success {
  padding: 60rpx 30rpx 0;
  position: relative;
}
.success .suc-bg {
  width: 100%;
  height: 644rpx;
  position: absolute;
  top: 68rpx;
  left: 0;
}
.suc-wrap {
  position: relative;
  padding: 36rpx 12rpx;
}
.suc-wrap .suc-img-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.suc-wrap .suc-img-wrap .suc-img {
  width: 180rpx;
  height: 180rpx;
}
.suc-wrap .suc-title {
  text-align: center;
  font-size: 52rpx;
  margin-top: 10rpx;
}
.suc-wrap .suc-intr {
  text-align: center;
  font-size: 32rpx;
  color: #888;
  margin-top: 30rpx;
}
.suc-wrap .suc-line {
  position: relative;
  height: 1px;
  margin: 56rpx 0;
}
.suc-wrap .suc-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 20rpx;
}
.suc-wrap .suc-item .item-title {
  font-size: 32rpx;
}
.suc-wrap .suc-item .item-txt {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #888;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
}
.suc-wrap .suc-item .item-txt .item-img {
  width: 44rpx;
  height: 44rpx;
  margin-right: 8rpx;
}
.sure-btn {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  background-color: #00a0e9;
  color: #fff;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
  margin-top: 120rpx;
}

