.extend-wraps {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  background-color: #f2f2f5;
}
.extend-wraps .extend-item {
  width: 690rpx;
  height: 150rpx;
  margin-top: 26rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  background-color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 30rpx 10rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.extend-wraps .extend-item .extend-headimg image {
  width: 90rpx;
  height: 90rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.extend-wraps .extend-item .extend-text {
  margin-left: 20rpx;
}
.extend-wraps .extend-item .extend-text .extend-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.extend-wraps .extend-item .extend-text .extend-title .extend-status {
  margin-left: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.extend-wraps .extend-item .extend-text .extend-title .extend-status image {
  width: 36rpx;
  height: 36rpx;
}
.extend-wraps .extend-item .extend-text .extend-time {
  margin-top: 16rpx;
  font-size: 24rpx;
}
.nodata {
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  color: #aaaaaa;
  font-size: 30rpx;
}

