.address-wraps {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #f2f2f5;
}
.address-full {
  margin-top: 26rpx;
  background-color: #fff;
  padding: 0 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.address-full .address-item {
  height: 210rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.address-full .address-item .address-person {
  width: 460rpx;
}
.address-full .address-item .address-person .person-name {
  font-size: 32rpx;
}
.address-full .address-item .address-person .person-name .person-phone {
  color: #aaa;
}
.address-full .address-item .address-person .person-location {
  margin-top: 20rpx;
  font-size: 24rpx;
}
.address-full .address-item .address-edit {
  width: 100rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #aaa;
  border-left: 1px solid #eeeeee;
  text-align: right;
  line-height: 50rpx;
}

